# UniBench Backend Configuration
# Copy this file to .env and fill in your actual values

# API Configuration
API_TITLE=UniBench Extraction API
API_VERSION=0.1.0
API_PREFIX=/api/v1
DEBUG=true

# CORS Configuration (comma-separated list)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# AI Provider Configuration
# Choose one or both providers and add your API keys

# Anthropic (Claude) API Key
# Get your key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API Key
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Default AI Provider (anthropic or openai)
DEFAULT_AI_PROVIDER=anthropic

# Default AI Model
# For Anthropic: claude-sonnet-4-20250514, claude-3-opus-20240229, claude-3-sonnet-20240229, claude-3-haiku-20240307
# For OpenAI: gpt-4-turbo-preview, gpt-4, gpt-3.5-turbo
DEFAULT_AI_MODEL=claude-sonnet-4-20250514

# File Upload Configuration
UPLOAD_DIR=./uploads
MAX_UPLOAD_SIZE=52428800

# Database Configuration (for future phases)
# DATABASE_URL=postgresql://user:password@localhost:5432/unibench
